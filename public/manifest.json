{
  "manifest_version": 3,
  "name": "LifeTab",
  "version": "0.0.2",
  "description": "Make your life's is simple!",
  "icons": {
    "16": "/assets/icons/UKN.png",
    "48": "/assets/icons/UKN.png",
    "128": "/assets/icons/UKN.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "geolocation",
    "alarms"
  ],
  "background": {
    "service_worker": "js/background.js",
    "type": "module"
  },
  "action": {
    "default_icon": {
      "48": "/assets/icons/UKN.png"
    }
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}